            <intent-filter>
                <action android:name="com.example.cybersafetyapp.PostInformationDetails" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <service android:name=".IntentServicePackage.IntentServiceGetJson" />
        <service
            android:name=".IntentServicePackage.IntentServiceInstagram"
            android:exported="false" />
        <service
            android:name=".IntentServicePackage.IntentServiceVine"
            android:exported="false" />

        <receiver android:name=".HelperClassesPackage.AlertReceiver" />

        <service
            android:name=".IntentServicePackage.IntentServiceNotification"
            android:exported="false" />
        <service
            android:name=".IntentServicePackage.IntentServiceServer"
            android:exported="false"></service>
    </application>

</manifest>