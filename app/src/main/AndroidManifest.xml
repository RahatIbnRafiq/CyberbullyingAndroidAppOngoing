<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cybersafetyapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeToCybersafetyApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Aboutus">
            <intent-filter>
                <action android:name="com.example.cybersafetyapp.Aboutus" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".LogIn">
            <intent-filter>
                <action android:name="com.example.cybersafetyapp.LogIn" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Register">
            <intent-filter>
                <action android:name="com.example.cybersafetyapp.Register" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Dashboard">
            <intent-filter>
                <action android:name="com.example.cybersafetyapp.Dashboard" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".AddNewProfile">
            <intent-filter>
                <action android:name="com.example.cybersafetyapp.AddNewProfile" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".SearchByUserName">
            <intent-filter>
                <action android:name="com.example.cybersafetyapp.SearchByUserName" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".SearchResultProfiles">
            <intent-filter>
                <action android:name="com.example.cybersafetyapp.SearchResultProfiles" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Notifications">
            <intent-filter>
                <action android:name="com.example.cybersafetyapp.Notifications" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".InstagramLogin">
            <intent-filter>
                <action android:name="com.example.cybersafetyapp.InstagramLogin" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".WaitForToken">
            <intent-filter>
                <action android:name="com.example.cybersafetyapp.WaitForToken" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".WaitForResults">
            <intent-filter>
                <action android:name="com.example.cybersafetyapp.WaitForResults" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".VineLogin">
            <intent-filter>
                <action android:name="com.example.cybersafetyapp.VineLogin" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".MonitoringProfileList">
            <intent-filter>
                <action android:name="com.example.cybersafetyapp.MonitoringProfileList" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name=".UserSocialNetworkInformationDetail">
            <intent-filter>
                <action android:name="com.example.cybersafetyapp.UserSocialNetworkInformationDetail" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <service android:name=".IntentServiceGetJson" />
        <service
            android:name=".IntentServiceInstagram"
            android:exported="false" />
        <service
            android:name=".IntentServiceVine"
            android:exported="false" />

    </application>

</manifest>